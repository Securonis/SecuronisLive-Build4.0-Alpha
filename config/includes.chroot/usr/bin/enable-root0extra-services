#!/bin/bash

# Enable Root0Extra services - Additional Security Hardening Services

# File system protection services
systemctl enable root0extra-fs-protected-regular.service

# Network security services
systemctl enable root0extra-network-tcp-fack.service
systemctl enable root0extra-network-igmp-max-memberships.service

echo "Root0Extra security services have been enabled. Please Reboot to apply changes."
